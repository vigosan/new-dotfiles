[user]
  email = {{ .email | quote }}
  name = {{ .name | quote }}
[color]
  ui = 1
  diff = auto
[merge]
  tool = vimdiff
[help]
  autocorrect = 1
[push]
  default = current
[core]
  ignorecase = false
  whitespace = -trailing-space
[rerere]
  autoupdate = true
  enabled = true
[pull]
  rebase = true
